nav#navbar.navbar.navbar-default.navbar-fixed-top(role='navigation')
    .container.navstyle
        .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
                span.sr-only Toggle navigation
            a.nav-link.navbar-brand(href='/#')
                img(src="/SVGs/logo.svg", width="160", height="48")
        .collapse.navbar-collapse.navbar-ex1-collapse
            ul.nav.navbar-right.navbar-nav(style='margin-right: -70px;')
                each module in [{page: 'components', title: 'COMPONENTS', menu: [{page: 'SPACERS', title: 'SPACERS'},{page: 'AMIRA', title: 'AMIRA SEATCLAMP'},{page: 'DHclamp', title: 'DH SEATCLAMP'},{page: 'PITADORA', title: 'PITADORA STEM'}]},{page: 'lifestyle', title: 'LIFESTYLE'},{page: 'dealers', title: 'DEALERS'},{page: 'aboutus', title: 'ABOUT US'}]
                    if module.menu & 1==2 
                        li.pict(class=module.page+((selection == module.page) ? 'A' : ''))
                            a.nav-link(href='/' + module.page, data-toggle="dropdown", role="button", aria-expanded="false") #{module.title}
                                ul(class="dropdown-menu", role="menu")
                                    each item in module.menu
                                        li(class='itemMenu')
                                            a.nav-link(href='/' + module.page +  '#' + item.page) #{item.title}
                    else
                        li.pict(class=module.page+((selection == module.page) ? 'A' : ''))
                            a.nav-link(href='/' + module.page) #{module.title}